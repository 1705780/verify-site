{
  "name": "verify-site",
  "version": "1.0.0",
  "author": {
    "name": "Dave Kozma",
    "email": "dkozma@adobe.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev:deploy": "gh-pages -d dist",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export && workbox injectManifest",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/lodash": "^4.14.161",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gh-pages": "^3.1.0",
    "nollup": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.1.0",
    "rollup": "^2.18.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^6.1.0",
    "spassr": "^1.1.2",
    "svelte-check": "^1.0.22",
    "tailwindcss": "^1.7.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@spectrum-web-components/icon": "^0.6.0",
    "@spectrum-web-components/icons-ui": "^0.3.0",
    "@spectrum-web-components/icons-workflow": "^0.3.3",
    "@sveltech/routify": "^1.9.7",
    "date-fns": "^2.16.1",
    "html-parse-stringify": "^2.0.0",
    "interactjs": "^1.9.22",
    "lodash": "^4.17.20",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "svelte": "^3.23.2",
    "svelte-css-vars": "^0.0.1",
    "svelte-preprocess": "^4.1.1",
    "workbox-cli": "^5.1.3"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
